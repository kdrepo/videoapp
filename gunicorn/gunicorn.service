[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/PBVIDEO/book_manager
ExecStart=/home/ubuntu/PBVIDEO/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.sock \
          book_manager/book_manager.wsgi:application
[Install]
WantedBy=multi-user.target
